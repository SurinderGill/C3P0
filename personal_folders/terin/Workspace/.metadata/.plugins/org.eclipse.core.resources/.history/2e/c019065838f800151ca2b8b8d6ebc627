import java.util.ArrayList;

public class StyleExpert implements Expert {

	public StyleExpert() {}

	@Override
	public Beer decision(String input, ArrayList<Beer> beers) {
		for (Beer b : beers) {
			if (b.type().equals(input)) {
				return b;
			}
		}
		return null;
	}
	
	public String toString() {
		return "I am the Style Expert.";
	}
}
